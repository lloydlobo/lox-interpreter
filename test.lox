// Scopes (1)
{
    var a = "first";
    print a; // "first".
}
{
    var a = "second";
    print a; // "second".
}

// Scopes (2)
var bar = "global bar";
var world = "global world";
var hello = "global hello";
{
  var bar = "outer bar";
  var world = "outer world";
  {
    var bar = "inner bar";
    print bar;
    print world;
    print hello;
  }
  print bar;
  print world;
  print hello;
}
print bar;
print world;
print hello;


// Scopes (3)
var baz = (91 * 16) - 61;
{
    var world = "quz" + "89";
    print world;
}
print baz;


// Block syntax
{
    var hello = 88;
    {
        var foo = 88;
        print foo;
    }
    print hello;
}

